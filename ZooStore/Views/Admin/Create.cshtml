@model CreateModel
<div class="bg-primary m-1 p-1 text-white"></div>
<div asp-validation-summary="All"></div>
<form asp-action="Create" method="post">
    <div class="form-group">
        <label asp-for="Name"></label>
        <input asp-for="Name" class="form-group" />
    </div>
    <div class="form-group">
        <label asp-for="Email"></label>
        <input asp-for="Email" class="form-group" />
    </div>
    <div class="form-group">
        <label asp-for="Password"></label>
        <input asp-for="Password" class="form-group" />
    </div>
    <button type="submit" class="btn btn-primary">Create</button>
    <a asp-action="UserList" class="btn btn-primary">Cancel</a>
</form>